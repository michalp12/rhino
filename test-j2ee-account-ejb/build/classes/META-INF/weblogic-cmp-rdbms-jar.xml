<?xml version="1.0"?>
<!DOCTYPE weblogic-rdbms-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB RDBMS Persistence//EN" "http://www.bea.com/servers/wls700/dtd/weblogic-rdbms20-persistence-700.dtd">
<weblogic-rdbms-jar>
  <weblogic-rdbms-bean>
    <ejb-name>Account</ejb-name>
    <data-source-name>cmpDataSource</data-source-name>
    <table-map>
      <table-name>ACCOUNT.ACCOUNT</table-name>
      <field-map>
        <cmp-field>accountNumber</cmp-field>
        <dbms-column>accountNumber</dbms-column>
      </field-map>
      <field-map>
        <cmp-field>balance</cmp-field>
        <dbms-column>balance</dbms-column>
      </field-map>
      <field-map>
        <cmp-field>accountType</cmp-field>
        <dbms-column>accountType</dbms-column>
      </field-map>
    </table-map>
  </weblogic-rdbms-bean>
  <create-default-dbms-tables>Disabled</create-default-dbms-tables>
</weblogic-rdbms-jar>
